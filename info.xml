<?xml version="1.0"?>
<extension key="frenchpostcodes" type="module">
  <file>frenchpostcodes</file>
  <name>frenchpostcodes</name>
  <description>frenchcodepostaux</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Renaud Fradin, Fawzy Elsam and Dewy Mercerais</author>
    <email>dev@all-in-appli.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://civicrm.org</url>
    <url desc="Documentation">https://github.com/allinappli/frenchpostcodes</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2021-12-14</releaseDate>
  <version>2.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.41</ver>
    <ver>5.77</ver>
  </compatibility>
  <comments>This extension searches for cities or French postal codes after entering a postal code or a city</comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Frenchpostcodes</namespace>
    <format>23.02.1</format>
    <angularModule>crmFrenchpostcodes</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
</extension>
